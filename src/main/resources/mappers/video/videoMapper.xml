<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.myungsang.myungsang_backend.video.iservice.VideoIService">
    <select id="getVideo" resultType="com.myungsang.myungsang_backend.video.vo.VideoVo">
        SELECT *
        FROM myungsang_db.videos
        WHERE id = #{id};
    </select>
</mapper>
